{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hexpat",
      "camelcase": "HexPat",
      "title": "HexPat",
      "scope": "source.hexpat",
      "file-types": [
        "hexpat"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^hexpat$",
      "class-name": "TreeSitterHexpat"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Hexpat grammar for tree-sitter",
    "authors": [
      {
        "name": "NathanSnail"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-hexpat"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
